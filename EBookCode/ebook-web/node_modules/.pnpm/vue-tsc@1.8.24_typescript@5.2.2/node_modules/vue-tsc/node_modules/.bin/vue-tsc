#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/vue-tsc@1.8.24_typescript@5.2.2/node_modules/vue-tsc/bin/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/vue-tsc@1.8.24_typescript@5.2.2/node_modules/vue-tsc/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/vue-tsc@1.8.24_typescript@5.2.2/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/vue-tsc@1.8.24_typescript@5.2.2/node_modules/vue-tsc/bin/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/vue-tsc@1.8.24_typescript@5.2.2/node_modules/vue-tsc/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/vue-tsc@1.8.24_typescript@5.2.2/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../bin/vue-tsc.js" "$@"
fi
