#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules/npm-run-all2/bin/run-s/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules/npm-run-all2/bin/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules/npm-run-all2/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules/npm-run-all2/bin/run-s/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules/npm-run-all2/bin/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules/npm-run-all2/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/npm-run-all2@6.1.1/node_modules:/home/melody/Documents/CS/DataBase/EBook/EBookCode/ebook-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/run-s/index.js" "$@"
else
  exec node  "$basedir/../../bin/run-s/index.js" "$@"
fi
